<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Colors-->
    <Color x:Key="PrimaryBlue">#17212b</Color>
    <Color x:Key="PrimaryBlack">#0e1621</Color>
    <Color x:Key="AccentBlueOne">#5288c1</Color>
    <Color x:Key="AccentBlackOne">#17212b</Color>
    <Color x:Key="AccentBlueTwo">#43acd9</Color>
    <Color x:Key="AccentBlackTwo">#262d37</Color>
    <Color x:Key="AccentBlackThree">#3e546a</Color>
    <Color x:Key="White">#ffffff</Color>
    <Color x:Key="WhiteDark">#969699</Color>
    <!--#endregion-->

    <!--#region Brushes-->
    <SolidColorBrush x:Key="White100" Color="{StaticResource White}" />
    <SolidColorBrush x:Key="Black100" Color="{StaticResource PrimaryBlack}" />
    <SolidColorBrush x:Key="Blue100" Color="{StaticResource PrimaryBlue}" />
    <SolidColorBrush x:Key="White200" Color="{StaticResource WhiteDark}" />
    <SolidColorBrush x:Key="Black200" Color="{StaticResource AccentBlackOne}" />
    <SolidColorBrush x:Key="Blue200" Color="{StaticResource AccentBlueOne}" />
    <SolidColorBrush x:Key="Blue300" Color="{StaticResource AccentBlueTwo}" />
    <SolidColorBrush x:Key="Black300" Color="{StaticResource AccentBlackTwo}" />
    <SolidColorBrush x:Key="Black400" Color="{StaticResource AccentBlackThree}" />
    <!--#endregion-->

</ResourceDictionary>